/* pages/index/index.wxss */
page {
    height: 100%;
}

.container {
    position: relative;
    overflow: hidden;
    height: 100%;

}

.bag {
    height: 1536px;
    position: absolute;
    width: 100%;
    z-index: -1;
}

.bag image {
    width: 100%;
    height: 768px;
    vertical-align: middle;
}

.Myfly image {
    width: 106rpx;
    height: 86rpx;

}

.Myfly {
    position: absolute;
    width: 106rpx;
    height: 86rpx;
    bottom: 80rpx;
    left: 50%;
   transform: translateX(-50%);
    margin: 0 auto;
    z-index: 1;
}
.bull{
    width: 10rpx;
    height: 10rpx;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    bottom: 200rpx;
    z-index: 2;
}
.bull image{
    width: 20rpx;
    height: 40rpx;

}
.arrfly{
    width: 98rpx;
    height: 76rpx;
}
.arrfly image{
    width: 98rpx;
    height: 76rpx;
    position: absolute;
}
.score{
    position: absolute;
    right: 0;
    top: 0;
    background: rgba(39, 39, 39, 0.5);
    color: white;
    padding: 20rpx;
    border-radius: 10rpx;
}